{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.tsx",
    "src/preload/*.d.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "preserveSymlinks": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@renderer/*": ["src/renderer/src/*"],
      "@atom/*": ["src/renderer/src/components/atom/*"],
      "@molecule/*": ["src/renderer/src/components/molecule/*"],
      "@organism/*": ["src/renderer/src/components/organism/*"],
      "@template/*": ["src/renderer/src/components/template/*"],
      "@hooks/*": ["src/renderer/src/hooks/*"],
      "@interfaces/*": ["src/renderer/src/interfaces/*"],
      "@styles/*": ["src/renderer/src/styles/*"]
    }
  }
}
