{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": ["src/renderer/src/env.d.ts", "src/renderer/src/**/*", "src/renderer/src/**/*.tsx", "src/preload/*.d.ts"],
  "compilerOptions": {
    "composite": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@renderer/*": ["src/renderer/src/*"],
      "@atoms/*": ["src/renderer/src/components/atoms/*"],
      "@molecules/*": ["src/renderer/src/components/molecules/*"],
      "@organisms/*": ["src/renderer/src/components/organisms/*"],
      "@templates/*": ["src/renderer/src/components/templates/*"],
      "@pages/*": ["src/renderer/src/components/pages/*"],
      "@hooks/*": ["src/renderer/src/hooks/*"],
      "@stores/*": ["src/renderer/src/hooks/stores/*"]
    }
  }
}
